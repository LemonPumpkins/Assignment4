<div class="jumbotron">
    <h1 class="display-3">everyones games</h1>
    <p class="lead">View the database of everyones games!</p>
    
</div>
{{#each games}}
<div class="card card-body">
    <h4>{{title}}</h4>
    <p>{{price}}</p>
    <p>{{description}}</p>
    <p>{{user}}</p>
</div>
{{else}}
<p>No Game Entries Found</p>
{{/each}}